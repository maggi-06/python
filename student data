# program to read student data
sno=int(input('enter student no:'))
sname=input('enter student name:')
group=input('enter group:')
gender=input('enter gender:')
phoneno=int(input('enter phno:'))
address=input('enter address:')
degreepercentage=input('enter percentage:')
print('sno:',sno)
print('sname:',sname)
print('group:',group)
print('gender:',gender)
print('phno',phoneno)
print('address:',address)
print('degree-percent:',degreepercentage)

output:
